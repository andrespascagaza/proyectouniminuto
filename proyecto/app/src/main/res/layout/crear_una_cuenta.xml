<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/correo"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/ingresar6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:text="¿Ya tienes tu cuenta?"
        android:textColor="#1E88E5"
        android:textSize="10sp"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="637dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="265dp"
        android:layout_height="30dp"
        android:text="Al registrate, acepta los terminos de servicio y la politica de privacidad "
        android:textColor="#039BE5"
        android:textDirection="firstStrongRtl"
        android:textIsSelectable="false"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="595dp" />

    <Button
        android:id="@+id/cerrar2"
        android:layout_width="77dp"
        android:layout_height="64dp"
        android:background="#00FFFFFF"
        android:backgroundTint="#3B8C3F"
        app:icon="@drawable/icons8_cerrar_ventana_50"
        app:iconTint="#3B8C3F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Crear una cuenta "
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.112" />

    <EditText
        android:id="@+id/nombres"
        android:layout_width="347dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Nombres"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="132dp" />

    <EditText
        android:id="@+id/apellidos"
        android:layout_width="347dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Apellidos"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="193dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="348dp"
        android:layout_height="49dp"
        android:ems="10"
        android:hint="Correo electronico"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="251dp" />

    <EditText
        android:id="@+id/telefono"
        android:layout_width="348dp"
        android:layout_height="41dp"
        android:ems="10"
        android:hint="Numero de telefono"
        android:inputType="phone"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="314dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="164dp"
        android:layout_height="41dp"
        android:backgroundTint="#B18316"
        android:text="Registrarse"
        android:textColor="#040202"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="545dp" />

    <EditText
        android:id="@+id/contraseña"
        android:layout_width="345dp"
        android:layout_height="49dp"
        android:ems="10"
        android:hint="Contraseña"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="370dp" />

    <EditText
        android:id="@+id/contraseña2"
        android:layout_width="345dp"
        android:layout_height="49dp"
        android:ems="10"
        android:hint="Repetir contraseña"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="433dp" />

    <EditText
        android:id="@+id/direccion"
        android:layout_width="344dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Dirección"
        android:inputType="textPostalAddress"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="494dp" />

</androidx.constraintlayout.widget.ConstraintLayout>